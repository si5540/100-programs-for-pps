#include<stdio.h>

int main()
{
    int n, i, p;
	
	long power=1;
    
	printf("Enter the number N=");
	scanf("%d", &n);
	
	printf("Enter the Power P=");
	scanf("%d", &p);
	
	for(i=1;i<=p;i++)
	  power = power * n;
	
	printf("The number %d raised to the power %d is %ld", n, p, power);
	
    return 0;
}
